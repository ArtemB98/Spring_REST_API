<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.example.demo.Entities.Regions">
        <table name="REGIONS" schema="HR" catalog=""/>
        <attributes>
            <id name="regionId">
                <column name="REGION_ID" precision="2147483646"/>
            </id>
            <basic name="regionName">
                <column name="REGION_NAME" nullable="false" length="25"/>
            </basic>
            <one-to-many name="countriesByRegionId" mapped-by="regionsByRegionId"
                         target-entity="com.example.demo.Entities.Countries"/>
        </attributes>
    </entity>
</entity-mappings>
